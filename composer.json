{
  "name": "moderntribe/event-tickets",
  "description": "Standalone Tickets Framework",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "events",
    "tickets"
  ],
  "homepage": "http://tri.be/shop/wordpress-events-calendar/",
  "license": "GPL-2.0",
  "require-dev": {
    "lucatume/wp-browser": "~2.0.5",
    "lucatume/function-mocker": "^1.0",
    "vlucas/phpdotenv": "^2.4",
    "lucatume/function-mocker-le": "^1.0",
    "spatie/phpunit-snapshot-assertions": "^1.2",
    "teamneusta/codeception-docker-chrome": "^1.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "post-install-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS"
    ],
    "post-update-cmd": [
      "PhantomInstaller\\Installer::installPhantomJS"
    ]
  }
}
